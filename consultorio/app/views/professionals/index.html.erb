<h1>SIii</h1>
<h1>Listing professionals</h1>

<table>
  <thead>
    <tr>
      <th>Professional</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @professionals.each do |professional| %>
      <tr>
        <td><%= professional.name %></td>
        
        <td><%= button_to "ELiminar", professional, :method => "delete" %></td>
       
        <%= button_to "Editar",edit_professional_path(professional), :method => "get" %>
      </tr>
    <% end %>
  </tbody>
</table>
<%= button_to "Agregar",new_professional_path,  :method => "get" %>
<br>


